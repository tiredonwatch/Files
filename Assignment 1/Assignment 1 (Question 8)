#include <stdio.h> 

int main(){ 

    for (int rows=1; rows<=5 ; rows++){ 

        for (int columns=1; columns<=rows; columns++){ 

            if((columns + rows)%2==0){ 

                printf("1"); 

            } 

            else{ 

                printf("0"); 

            } 

        } 

        printf("\n"); 

    } 

 return 0; 

} 
