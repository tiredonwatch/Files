#include <stdio.h> 

int main() { 
    int NumberStudents; 
    printf("Enter the Number of Students:"); 
    scanf("%d",&NumberStudents); 
    float marks[NumberStudents]; 
    for(int i=0; i<NumberStudents ; i++){ 

            printf("Enter marks of student %d:",i+1); 
            scanf("%f",&marks[i]); 
    } 

    printf("\n"); 
    printf("**************************"); 
    printf("\n"); 
    for(int k=0; k<NumberStudents; k++){ 

        if((int)marks[k]%2==0){ 
            printf("The score of roll number %d is even\n",k+1); 

        } 

        else { 
            printf("The score of roll number %d is odd\n",k+1); 
        } 
    } 

        printf("**************************"); 

    return 0; 

} 

 

 
