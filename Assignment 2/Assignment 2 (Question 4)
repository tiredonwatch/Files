#include <stdio.h> 

int main() { 
    int NumberStudents; 
    printf("Enter the Number of Students:"); 
    scanf("%d",&NumberStudents); 
    int marks[NumberStudents]; 
    for(int i=0; i<NumberStudents ; i++){ 
            printf("Enter marks of student %d:",i+1); 
            scanf("%d",&marks[i]); 

    } 
    printf("\n"); 
    printf("**************************"); 
    printf("\n"); 
    int count=0; 
    printf("Roll Number:\n"); 
    for(int k=0 ; k<NumberStudents ; k++){ 
        if (marks[k]==99){ 
            printf("%d\n",k+1); 
            count+=1; 
        } 

    } 

    printf("Scored %d marks",99); 
    printf("\n %d students scored %d marks",count,99); 
    printf("\n**************************"); 
    return 0; 

} 

 
